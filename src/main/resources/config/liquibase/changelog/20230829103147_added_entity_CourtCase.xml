    <!--
        Added the entity CourtCase.
    -->
    <changeSet id="20230829103147-1" author="jhipster">
        <createTable tableName="court_case">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            
            <column name="file_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="unique_file_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="sr_no" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
             <column name="bank_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="taluka_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="account_no" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="name_of_defaulter" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="address" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="loan_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="loan_amount" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="loan_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="term_of_loan" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="interest_rate" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="installment_amount" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="total_credit" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="balance" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="interest_paid" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="penal_interest_paid" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="due_amount" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="due_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="due_interest" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="due_penal_interest" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="due_more_interest" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="interest_recivable" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="taaran" type="TEXT">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_one" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_one_address" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_two" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_two_address" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="first_notice_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="second_notice_date" type="date">
                <constraints nullable="true" />
            </column>
            
            
            <!-- english data -->
            
             <column name="sr_no_en" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="bank_name_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="taluka_name_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="account_no_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="name_of_defaulter_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="address_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="loan_type_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="loan_amount_en" type="double">
                <constraints nullable="true" />
            </column>
            
            <column name="term_of_loan_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="interest_rate_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="installment_amount_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="total_credit_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="balance_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="interest_paid_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="penal_interest_paid_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="due_amount_en" type="double">
                <constraints nullable="true" />
            </column>
           
            <column name="due_interest_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="due_penal_interest_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="due_more_interest_en" type="double">
                <constraints nullable="true" />
            </column>
            <column name="interest_recivable_en" type="double">
                <constraints nullable="true" />
            </column>
             <column name="taaran_en" type="TEXT">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_one_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_one_address_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_two_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="gaurentor_two_address_en" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            
            <column name="created_by" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp"/>
            <column name="last_modified_by" type="varchar(50)"/>
            <column name="last_modified_date" type="timestamp"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

