    <!--
        Added the entity KmCrops.
    -->
    <changeSet id="20240105061859-1" author="jhipster">
        <createTable tableName="km_crops">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="crop_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="crop_name_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="hector" type="double">
                <constraints nullable="true" />
            </column>

            <column name="hector_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="are" type="double">
                <constraints nullable="true" />
            </column>

            <column name="are_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="prvious_amt" type="double">
                <constraints nullable="true" />
            </column>

            <column name="previous_amt_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="demand" type="double">
                <constraints nullable="true" />
            </column>

            <column name="demand_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="society" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="society_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="bank_amt" type="double">
                <constraints nullable="true" />
            </column>

            <column name="bank_amt_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="no_of_tree" type="double">
                <constraints nullable="true" />
            </column>

            <column name="no_of_tree_mr" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="km_details_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here, do not remove-->
